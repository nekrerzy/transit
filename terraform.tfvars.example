# Example terraform.tfvars file
# Copy this to terraform.tfvars and customize for your environment

subscription_id                   = "your-azure-subscription-id-here"
private_endpoint_subnet_id        = "/subscriptions/your-sub-id/resourceGroups/rg-network-dev-incp-uaen-001/providers/Microsoft.Network/virtualNetworks/vnet-name/subnets/SNET-PE"
virtual_network_id                = "/subscriptions/your-sub-id/resourceGroups/rg-network-dev-incp-uaen-001/providers/Microsoft.Network/virtualNetworks/vnet-name"
postgresql_delegated_subnet_id    = "/subscriptions/your-sub-id/resourceGroups/rg-network-dev-incp-uaen-001/providers/Microsoft.Network/virtualNetworks/vnet-name/subnets/subnet-postgres"
postgresql_private_dns_zone_id    = "/subscriptions/your-sub-id/resourceGroups/rg-network-dev-incp-uaen-001/providers/Microsoft.Network/privateDnsZones/psql-bain-dev-incp-uaen-001.private.postgres.database.azure.com"
postgresql_admin_password         = "YourSecurePassword123!"
component                         = "app"
environment                       = "dev"
region                            = "uaen" 
sequence                          = "002"
location                          = "UAE North"

common_tags = {
  Environment = "dev"
  Project     = "bain"
  ManagedBy   = "terraform"
  CostCenter  = "IT"
}